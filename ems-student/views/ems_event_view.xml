<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="ems_envent_inherit" model="ir.ui.view">
        <field name="name">ems.eventview.form.inherit</field>
        <field name="model">ems.event</field>
        <field name="inherit_id" ref="ems_events.ems_event_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//sheet" position="inside">
                <notebook>
                    <page name="particpant" string="Particpants">
                        <field name="student_ids" >
                            <tree editable="top">
                                <field name="state" invisible="1" />
                                <field name="name"/>
                                <field name="parent_id" />
                                <field name="nic" />
                                <field name="class_id"/>
                            </tree>
                        </field>
                    </page>
                </notebook>
            </xpath>
        </field>
    </record>

</odoo>
